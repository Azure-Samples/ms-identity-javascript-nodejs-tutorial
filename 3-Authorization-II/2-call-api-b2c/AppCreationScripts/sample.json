{
    "Sample": {
        "Title": "A Node.js & Express web App calling a custom web API on Azure AD B2C using MSAL Node",
        "Level": 200,
        "Client": "Node.js & Express Web App",
        "Service": "Node.js & Express Web API",
        "RepositoryUrl": "ms-identity-javascript-nodejs-tutorial",
        "Endpoint": "AAD v2.0",
        "Provider": "B2C"
    },
    "AADApps": [
        {
            "Id": "Service",
            "Name": "ExpressWebApi-c3s2",
            "Kind": "WebApi",
            "Audience": "AzureADMyOrg",
            "HomePage": "http://localhost:5000/"
        },
        {
            "Id": "Client",
            "Name": "ExpressWebApp-c3s2",
            "Kind": "WebApp",
            "Audience": "AzureADMyOrg",
            "HomePage": "http://localhost:4000/",
            "ReplyUrls": "http://localhost:4000/redirect",
            "PasswordCredentials": "Auto",
            "RequiredResourcesAccess": [
                {
                "Resource": "Service",
                "DelegatedPermissions": [ "access_as_user" ]
                }
            ]
        }
    ],
    "CodeConfiguration": [
        {
            "App": "Service",
            "SettingKind": "JSON",
            "SettingFile": "\\..\\WebAPI\\auth.json",
            "Mappings": [
              {
                "key": "clientId",
                "value": ".AppId"
              },
              {
                "key": "tenantId",
                "value": "$tenantId"
              }
            ]
        },
        {
            "App": "Client",
            "SettingKind": "JSON",
            "SettingFile": "\\..\\WebApp\\auth.json",
            "Mappings": [
            {
                "key": "clientId",
                "value": ".AppId"
            },
            {
                "key": "tenantId",
                "value": "$tenantId"
            },
            {
                "key": "clientSecret",
                "value": ".AppKey"
            },
            {
                "key": "redirectUri",
                "value": ".ReplyUrls"
            },
            {
                "key": "postLogoutRedirectUri",
                "value": ".HomePage"
            }
            ]
        }
    ]
  }