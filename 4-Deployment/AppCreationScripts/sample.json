{
    "Sample": {
        "Title": "A Node.js & Express web app calling a custom web API on Azure AD using MSAL Node",
        "Level": 200,
        "Client": "Node.js & Express web app",
        "Service": "Node.js & Express web API",
        "RepositoryUrl": "ms-identity-javascript-nodejs-tutorial",
        "Endpoint": "AAD v2.0"
    },
    "AADApps": [
        {
            "Id": "service",
            "Name": "msal-node-webapi",
            "Kind": "WebApi",
            "Audience": "AzureADMyOrg",
            "HomePage": "http://localhost:5000/",
            "PasswordCredentials": "Auto"
        },
        {
            "Id": "client",
            "Name": "msal-node-webapp",
            "Kind": "WebApp",
            "Audience": "AzureADMyOrg",
            "HomePage": "http://localhost:4000/",
            "ReplyUrls": "http://localhost:4000/redirect",
            "PasswordCredentials": "Auto",
            "RequiredResourcesAccess": [
                {
                    "Resource": "service",
                    "DelegatedPermissions": [
                        "access_as_user"
                    ]
                }
            ]
        }
    ],
    "CodeConfiguration": [
        {
            "App": "service",
            "SettingKind": "JSON",
            "SettingFile": "\\..\\WebAPI\\config.json",
            "Mappings": [
                {
                    "key": "clientId",
                    "value": ".AppId"
                },
                {
                    "key": "tenantId",
                    "value": "$tenantId"
                },
                {
                    "key": "clientSecret",
                    "value": ".AppKey"
                }
            ]
        },
        {
            "App": "client",
            "SettingKind": "JSON",
            "SettingFile": "\\..\\WebApp\\appSettings.json",
            "Mappings": [
                {
                    "key": "clientId",
                    "value": ".AppId"
                },
                {
                    "key": "tenantId",
                    "value": "$tenantId"
                },
                {
                    "key": "clientSecret",
                    "value": ".AppKey"
                },
                {
                    "key": "redirectUri",
                    "value": ".ReplyUrls"
                },
                {
                    "key": "postLogoutRedirectUri",
                    "value": ".HomePage"
                },
                {
                    "key": "endpoint",
                    "value": "service.HomePage"
                },
                {
                    "key": "scopes",
                    "value": "service.Scope"
                }
            ]
        }
    ]
}